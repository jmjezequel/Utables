	Year = 2010}

@article{acher-collet-etal:2010e,
	Abstract = {{  The development of scientific workflows is evolving towards the systematic use of service oriented architectures, enabling the composition of dedicated and highly parameterized software services into processing pipelines. Building consistent workflows then becomes a cumbersome and error-prone activity as users cannot manage such large scale variability. This paper presents a rigorous approach in which techniques from Soft- ware Product Line (SPL) engineering are reused and extended to manage variability in service and workflow descriptions, so that composition can be facilitated while en- suring consistency. Services are organized in an SPL acting as a rich catalog structured according to the common and variable concerns captured for all services. By relying on sound merging techniques on the feature models that make up an SPL, reasoning about the compatibility between connected services is made possible. Moreover, an entire workflow can be seen as a multiple SPL (i.e., a composition of several SPLs) and when services are configured within, the propagation of variability choices is automated and the user is assisted in obtaining a consistent workflow. The approach proposed is completely supported by a combination of dedicated tools and languages. Illustrations and experimental validations are provided using medical imaging pipelines, which are representative of current scientific workflows in many domains.
}},
	Author = {Acher, Mathieu and Collet, Philippe and Gaignard, Alban and Lahire, Philippe and Montagnat, Johan and France, Robert B.},
	Date-Added = {2011-05-30 22:47:03 +0200},
	Date-Modified = {2011-05-30 22:47:03 +0200},
	Journal = {Software Quality Journal - Special issue on Quality Engineering for Software Product Lines},
	Lang = {english},
	Month = nov,
	Note = {submitted: currently under review},
	Sorte = {revue int},
	Title = {{Managing Variability in Scientific Workflows}},
	Year = 2010}

